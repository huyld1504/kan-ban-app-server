{
     "name": "server",
     "version": "0.0.0",
     "private": true,
     "scripts": {
          "start": "nodemon ./bin/www",
          "test:keploy": "jest --coverage --coverageReporters=text --coverageReporters=cobertura --collectCoverageFrom='./**/*.{js,jsx,ts,tsx}'",
          "test:coverage": "jest --coverage --coverageReporters=lcov",
          "test:ci": "npm run test:coverage && curl https://deepsource.io/cli | sh && ./bin/deepsource report --analyzer test-coverage --key javascript --value-file ./coverage/lcov.info"
     },
     "dependencies": {
          "cookie-parser": "~1.4.4",
          "cors": "^2.8.5",
          "crypto-js": "^4.2.0",
          "debug": "~2.6.9",
          "dotenv": "^16.4.5",
          "express": "~4.16.1",
          "express-validator": "^7.2.0",
          "jsonwebtoken": "^9.0.2",
          "k6": "^0.0.0",
          "mongoose": "^8.5.2",
          "morgan": "~1.9.1",
          "nodemon": "^3.1.4"
     },
     "devDependencies": {
          "chai": "^4.3.4",
          "chai-http": "^5.1.2",
          "jest": "^29.7.0",
          "mocha": "^11.5.0",
          "mongoose-mock": "^0.4.0",
          "nyc": "^17.1.0",
          "sinon": "^20.0.0",
          "sinon-chai": "^3.7.0",
          "supertest": "^7.1.1"
     }
}